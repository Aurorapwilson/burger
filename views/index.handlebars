<h1>Burgers</h1>

<h2>Eat the burger!</h2>

<ul>
  {{#each burger}}
  {{#unless devoured=true}}
  {{/unless}}
  {{/each}}
</ul>



<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>


  <button type="submit">Add Cat</button>
</form>